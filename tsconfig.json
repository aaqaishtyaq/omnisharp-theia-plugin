{
    "compilerOptions": {
        "sourceMap": true,
		"moduleResolution": "node",
		"alwaysStrict": true,
		"noImplicitAny": true,
		"skipLibCheck": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noFallthroughCasesInSwitch": true, 
        "module": "commonjs",
        "target": "es6",
        "lib": [
            "es6",
            "webworker"
        ],
        "rootDir": "src",
        "outDir": "lib"
    },
    "include": [
        "src"
    ]
}
